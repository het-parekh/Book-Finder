{
    "version": 2,
    "builds": [
      {
        "src": "./backend/app.js", 
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/build",   
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/backend/app.js"
      }
    ]
  }